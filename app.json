{
  "expo": {
    "name": "TecnoBooth",
    "slug": "TecnoBooth",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./resources/img/favicon.png",
    "splash": {
      "image": "./resources/img/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#041F2E"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#041F2E"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./resources/img/favicon.png",
        "backgroundColor": "#041F2E"
      }
    },
    "web": {
      "favicon": "./resources/img/favicon.png"
    }
  }
}
